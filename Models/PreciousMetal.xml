 <!-- Prop #1 (forward) -->
 <animation>
  <type>select</type>
  <object-name>Prop1</object-name>
  <condition>
    <less-than>
     <property>engines/engine[0]/rpm</property>
     <value>400</value>
    </less-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>PropBlur1</object-name>
  <condition>
   <and>
    <less-than>
     <property>engines/engine[0]/rpm</property>
     <value>1200</value>
    </less-than>
    <greater-than>
     <property>engines/engine[0]/rpm</property>
     <value>400</value>
    </greater-than>
  </and>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>PropDisk1</object-name>
  <condition>
    <greater-than>
     <property>engines/engine[0]/rpm</property>
     <value>1200</value>
    </greater-than>
  </condition>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>Prop1</object-name>
  <object-name>PropBlur1</object-name>
  <property>engines/engine[0]/rpm</property>
  <center>
   <x-m>-3.21</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


