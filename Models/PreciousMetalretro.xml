 <animation>
  <type>select</type>
  <object-name>LTire</object-name>
  <object-name>LMainGearDoorOuter</object-name>
  <object-name>LMainGearDoorInner</object-name>
  <object-name>LMainGearStrut</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0.05</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>RTire</object-name>
  <object-name>RMainGearDoorOuter</object-name>
  <object-name>RMainGearDoorInner</object-name>
  <object-name>RMainGearStrut</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[1]/position-norm</property>
    <value>0.05</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>MainGearWellCover</object-name>
  <condition>
   <less-than>
    <property>gear/gear[1]/position-norm</property>
    <value>0.05</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>TailWheel</object-name>
  <object-name>TailWheelStrut</object-name>
  <object-name>TailWheelStrutLower</object-name>
  <object-name>LTailWheelDoor</object-name>
  <object-name>RTailWheelDoor</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[2]/position-norm</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>

 <!-- Tail gear retract -->
 <animation>
  <type>rotate</type>
  <object-name>TailWheelStrut</object-name>
  <object-name>TailWheelStrutLower</object-name>
  <object-name>TailWheel</object-name>
  <property>gear/gear[0]/position-norm</property>
  <factor>75</factor>
  <offset-deg>-75</offset-deg>   
  <center>
   <x-m> 3.3</x-m>
   <y-m>   0</y-m>
   <z-m>-0.83</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <!-- Tail wheel steering -->
 <animation>
  <type>rotate</type>
  <object-name>TailWheelStrutLower</object-name>
  <object-name>TailWheel</object-name>
  <property>sim/multiplay/generic/float[2]</property>
  <factor>-1</factor>
  <center>
   <x-m> 4.06</x-m>
   <y-m> 0.00</y-m>
   <z-m>-0.73</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>


 <!-- Prop #1 (forward) -->
 <animation>
  <type>select</type>
  <object-name>PropFwd</object-name>
  <condition>
    <less-than>
     <property>engines/engine[0]/rpm</property>
     <value>400</value>
    </less-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>PropBlur1</object-name>
  <condition>
   <and>
    <less-than>
     <property>engines/engine[0]/rpm</property>
     <value>1200</value>
    </less-than>
    <greater-than>
     <property>engines/engine[0]/rpm</property>
     <value>400</value>
    </greater-than>
  </and>
  </condition>
 </animation>

